{
    "explorer": {
        "theme": {
            "default": "webdows",
            "extraCSS": ""
        },
		"startup": [
			"webdows/config/explorer.startup.js",
			"webdows/welcome.js",
			"webdows/resources/explorer/animation.js"
		]
    },
    "system": {
		"webldr": {
			"exclude": [
				"index.html",
				"Public/",
				"programs/",
				"webdows/resources/icons/",
				"webdows/config/",
				"webdows/system32/"
			]
		},
		"startup": [
			"programs/belowaverage-org/weblogon/weblogon.js"
		],
        "files": {
            "ext": {
                "txt": {
                    "icon": "webdows/resources/icons/note.ico",
                    "defaultProgram": "webdows/notepad.js"
                },
                "js": {
                    "icon": "webdows/resources/icons/exei.ico"
                },
                "png": {
                    "icon": "webdows/resources/icons/pngi.ico",
                    "defaultProgram": ""
                },
                "gif": {
                    "icon": "webdows/resources/icons/gifi.ico",
                    "defaultProgram": ""
                },
                "jpg": {
                    "icon": "webdows/resources/icons/jpgi.ico",
                    "defaultProgram": ""
                },
                "ico": {
                    "icon": "webdows/resources/icons/bmpi.ico",
                    "defaultProgram": ""
                }
            }
        }
    },
	"software": {
		"belowaverage-org": {
			"weblogon": {
				"domain": "belowaverage.org",
				"registry": "https://api.belowaverage.org/v1/webdows/registry/"
			}
		}
	}
}